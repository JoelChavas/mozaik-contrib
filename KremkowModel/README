python run.py nest 1 param_read_LGN_spikes//defaults 'reading_LGN_spikes'
#It will read the LGN spikes from the datastore stored in FFI_full_model____
python run.py hardware.brainscales 1 param_hardware_read_LGN_spikes/defaults 'hardware_reading_LGN_spikes'
python run.py nest 1 param_nest_like_hardware/defaults 'nest_like_hardware'